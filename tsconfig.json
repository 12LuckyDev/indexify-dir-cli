{
	"compilerOptions": {
		"outDir": "dist",
		"lib": [
			"es2023"
		],
		"module": "nodenext",
		"target": "es2022",
		"strict": true,
		"noImplicitAny": true,
		"strictFunctionTypes": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noUncheckedIndexedAccess": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"moduleResolution": "nodenext",
	},
	"include": [
		"src"
	]
}